generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id    Int     @id @default(autoincrement())
  name  String?
  theme  String?
  thumbnail String?
}

model File {
  id       Int      @id @default(autoincrement())
  url      String   @unique
  userId   Int
  uploadedAt DateTime @default(now())
}